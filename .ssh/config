# Defaults for Windows hosts.
Host windows-*

    # Set default user.
    User corey

# Defaults for all hosts.
Host *

    # Set default user.
    User cholland1989

    # Send keep-alive every 60 seconds.
    ServerAliveInterval 60

    # Retry keep-alive up to 3 times.
    ServerAliveCountMax 3

    # Hide MOTD banner.
    LogLevel QUIET
